<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.pesc.edexchange.v1_0">
   
   <class name="DeliveryMethod" table="delivery_methods" catalog="${hibernate.catalog}">
    <meta attribute="class-description">
        This table is derived from the DeliveryMethod schema defined in EdExchangeEnvelope_v1.0.8.xsd
    </meta>
    <id type="int" column="id">
        <generator class="native" />
    </id>
    <property name="method" column="method" not-null="true" type="string" />
    
   </class>
</hibernate-mapping>