<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.pesc.edexchange.v1_0">
   
   <class name="OrganizationContact" table="organization_contact" catalog="${hibernate.catalog}">
    <meta attribute="class-description">
        This table is derived from the OrganizationContact schema defined in EdExchangeEnvelope_v1.0.7.xsd
    </meta>
    <id type="int" column="contact_id">
        <generator class="native" />
    </id>
    <property name="contactName" column="contact_name" not-null="true" type="string" />
    <property name="contactTitle" column="contact_title" not-null="true" type="string" />
    <property name="contactType" column="contact_type" not-null="true" type="string" />
    <property name="email" column="email" not-null="true" type="string" />
    <property name="phone1" column="phone_1" not-null="true" type="string" />
    <property name="phone2" column="phone_2" type="string" />
    <property name="streetAddress1" column="street_address_1" not-null="true" type="string" />
    <property name="streetAddress2" column="street_address_2" not-null="true" type="string" />
    <property name="streetAddress3" column="street_address_3" type="string" />
    <property name="streetAddress4" column="street_address_4" type="string" />
    <property name="city" column="city" not-null="true" type="string" />
    <property name="state" column="state" not-null="true" type="string" />
    <property name="zip" column="zip" not-null="true" type="string" />
    <property name="country" column="country" not-null="true" type="string" />
    
    <many-to-one name="directory" column="directory_id" class="Organization" not-null="true" lazy="false" />
    
    <property name="createdTime" column="created_time" type="timestamp" update="false" insert="false" generated="always" />
	<property name="modifiedTime" column="modified_time" type="timestamp" update="false" insert="false" generated="always" />
    
   </class>
</hibernate-mapping>