<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.pesc.edexchange.v1_0">
   
	<class name="EntityCode" table="organization_entity_code" catalog="${hibernate.catalog}">
		<meta attribute="class-description">
			This table is derived from the EntityCode schema defined in EdExchangeEnvelope_v1.0.7.xsd
		</meta>
		<id name="id" column="id" type="integer">
			<generator class="identity" />
		</id>
		<property name="code" column="entity_code" type="integer" not-null="true" />
		<property name="description" column="description" not-null="true" type="string" />
		<property name="createdTime" column="created_time" type="timestamp" update="false" insert="false" generated="always" />
		<property name="modifiedTime" column="modified_time" type="timestamp" update="false" insert="false" generated="always" />
	</class>
</hibernate-mapping>