<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.pesc.edexchange.v1_0">
   
   <class name="DocumentFormat" table="document_format" catalog="pesc_edexchange">
    <meta attribute="class-description">
        This table is derived from the DocumentFormat schema defined in EdExchangeEnvelope_v1.0.7.xsd
    </meta>
   	<id name="id" column="id" type="integer">
        <generator class="identity" />
   	</id>
    <property name="formatName" column="format_name" not-null="true" type="string" length="10" />
    <property name="formatDescription" column="format_description" type="string" />
    <property name="formatInuseCount" column="format_inuse_count" type="integer" />
    <property name="createdTime" column="created_time" type="timestamp" update="false" insert="false" generated="always" />
    <property name="modifiedTime" column="modified_time" type="timestamp" update="false" insert="false" generated="always" />
    
   </class>
</hibernate-mapping>