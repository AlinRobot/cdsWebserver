                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="getCDSBatchResponse">
                <xs:sequence>
                    <xs:element minOccurs="0" name="getCDSBatch" type="xs:string"/>
                </xs:sequence>
            </xs:complexType>
            
            <xs:element name="getDeliveryLocation">
