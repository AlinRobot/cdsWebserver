            </xs:complexType>
            <xs:complexType name="getCDSBatch">
                <xs:sequence>
                    <xs:element minOccurs="0" name="psisCode" type="xs:string"/>
                    <xs:element minOccurs="0" name="username" type="xs:string"/>
